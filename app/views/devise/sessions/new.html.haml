.login-body
  %article.container-login.center-block
    %section
      %ul#top-bar.nav.nav-tabs.nav-justified
        %li.active
          = link_to "Log in", new_session_path(resource_name)
        %li
          = link_to "Sign up", new_registration_path(resource_name)
      .tab-content.tabs-login.col-lg-12.col-md-12.col-sm-12.cols-xs-12
        #login-access.tab-pane.fade.active.in
          %h2
            %i.glyphicon.glyphicon-log-in
            Access
          %form.form-horizontal{"accept-charset" => "utf-8", :autocomplete => "off", :method => "post", :role => "form"}
            .form-group
              %label.sr-only{:for => "login"} Email
              %input#login_value.form-control{:name => "login", :placeholder => "Email", :tabindex => "1", :type => "text", :value => ""}/
            .form-group
              %label.sr-only{:for => "password"} Password
              %input#password.form-control{:name => "password", :placeholder => "Password", :tabindex => "2", :type => "password", :value => ""}/
            .checkbox
              %label.control-label{:for => "remember_me"}
                %input#remember_me{:name => "remember_me", :tabindex => "3", :type => "checkbox", :value => "1"}/
                Remeber Me
            %br/
            .form-group
              %button#submit.btn.btn-lg.btn-primary{:name => "log-me-in", :tabindex => "5", :type => "submit"} submit
