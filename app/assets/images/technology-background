          $("nav.pager a").on('click', function() {
            var term = $('#search-term').val();
              var pu_url = "https://shutterstock.7eer.net/c/38919/111120/1305?subId1=pagination&subId2="+encodeURI(term)+"&subId3="+ $.cookies.get('ga-client-id') +"&u=http%3A%2F%2Ffootage.shutterstock.com%2Fsearch%2F"+term.replace(/ /gi,"%2B")+"&sharedid=www.videezy.com"
            if (!$.cookies.get('popped')) {
              $.cookies.set('popped', '1', {hoursToLive: .5});
              underpop(pu_url);
              track("sstk_popup", 'pagination', term);
            }
